                    , Run MultiCpu ["-t","<total0>% <total1>% <total2>% <total3>%", "-L", "40", "-H", "60", "-l", "lightblue", "-n", "white", "-h", "red"] 10
                    , Run CpuFreq ["-t", "<cpu0> <cpu1>", "-L", "0", "-H", "2", "-l", "lightblue", "-n","white", "-h", "red"] 50
