                    , Run Swap [] 10
                    , Run Wireless "wlan0" ["-t","wlan0: <quality>%","-L","0","-H","32","--normal","green","--high","red"] 300

                    , Run BatteryP ["BAT0", "BAT1"] ["-t", "<timeleft>"
                                        , "-L", "10", "-H", "80"
                                        , "-l", "red", "-h", "green"
